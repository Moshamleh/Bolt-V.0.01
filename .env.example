# ðŸš— BOLT AUTO - Environment Variables Template
# ===============================================

# OpenAI API Configuration
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# RAG Backend URL
VITE_RAG_BACKEND_URL=http://localhost:5000

# Stripe Payment Configuration
VITE_STRIPE_PUBLIC_KEY=your_stripe_public_key_here

# Application Environment
NODE_ENV=development
VITE_APP_ENV=development

# Security Configuration
VITE_ENABLE_SECURITY_LOGGING=true
VITE_RATE_LIMIT_ENABLED=true

# Feature Flags
VITE_ENABLE_BETA_FEATURES=false
VITE_ENABLE_ADVANCED_DIAGNOSTICS=true
VITE_ENABLE_BULK_UPLOAD=false
VITE_ENABLE_VIDEO_CALLS=false

# Analytics & Monitoring
VITE_ANALYTICS_ENABLED=true
VITE_ERROR_REPORTING_ENABLED=true

# AI Configuration
VITE_AI_MODEL=gpt-4
VITE_AI_MAX_TOKENS=4000
VITE_AI_TEMPERATURE=0.7

# WebRTC Configuration (for video calls)
VITE_WEBRTC_STUN_SERVER=stun:stun.l.google.com:19302
VITE_WEBRTC_TURN_SERVER=your_turn_server_here

# File Upload Configuration
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=jpg,jpeg,png,pdf,mp4